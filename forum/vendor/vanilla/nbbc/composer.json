	"license": "BSDv2",
	"authors": [
		{
			"name": "Todd Burry",
			"email": "todd@vanillaforums.com"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:vanilla/garden.git"
		}
	],
	"require": {
		"php": ">=5.4.0"
	},
	"autoload": {
